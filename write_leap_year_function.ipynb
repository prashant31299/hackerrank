    "def is_leap(year):\n",
    "    leap = False\n",
    "    if year % 400==0 and year % 100==0:\n",
    "        leap = True\n",
    "    elif year % 4==0 and year % 100 !=0:\n",
    "        leap =True\n",
    "\n",
    "    # Write your logic here\n",
    "    \n",
    "    return leap\n",
    "\n",
    "year = int(input())\n",
    "print(is_leap(year))"
